<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_gin" model="ir.sequence">
            <field name="name">Good Issue Note Sequence</field>
            <field name="code">stock.gin</field>
            <field name="prefix">GIN/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_rfi" model="ir.sequence">
            <field name="name">Stock RFI Sequence</field>
            <field name="code">stock.rfi</field>
            <field name="prefix">RFI/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>